    "keywords": ["Link", "url", "encryption", "decryption", "id", "security"],
    "homepage": "https://github.com/NAYZO/NzoUrlEncryptorBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ala Eddine Khefifi",
            "email": "alakhefifi@gmail.com",
            "website": "www.alakhefifi.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1"
    },
    "require-dev": {
        "twig/twig": "~1.5",
        "symfony/yaml": "~2.1"
    },    
    "autoload": {
        "psr-0": { "Nzo\\UrlEncryptorBundle": "" }
    },
    "target-dir": "Nzo/UrlEncryptorBundle"
}